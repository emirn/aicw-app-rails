{
  "name": "@blogpostgen/cli",
  "version": "1.0.0",
  "description": "CLI for BlogPostGen AI-powered article generation",
  "bin": {
    "blogpostgen": "./bin/blogpostgen"
  },
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && cp -r src/config dist/",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "seo",
    "content",
    "ai",
    "cli",
    "articles",
    "blog"
